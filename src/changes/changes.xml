<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://maven.apache.org/changes/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/changes/1.0.0 http://maven.apache.org/xsd/changes-1.0.0.xsd">
    <properties>
        <title>Cambios de ManagePdfServer</title>
        <author email="jmiraval@zytrust.com">Jacinto Miraval</author>
    </properties>
    <body>

        <release version="1.6.0" date="2021-11-15" description="Soporte para operar pdf sin guardar en disco">
            <action dev="jmiraval" type="add">
               Soporte para operar pdfs sin guardar en disco
            </action>
        </release>

        <release version="1.3.2" date="2020-12-21" description="Quitado de extension .pdf">
            <action dev="jmiraval" type="add">
               Se cambia el nombre del documento descencriptado quitando la extension .pdf para que no se duplique al firmar
            </action>
        </release>
        
		<release version="1.2.0" date="2021-01-06" description="Carga de archivo con json">
            <action dev="jmiraval" type="add">
                Se agrega método para cargar archivos en base 64
            </action>

        </release>
        
        <release version="1.1.0" date="2021-01-04" description="Insertar marca agua">
            <action dev="jmiraval" type="add">
                Se agregó método para insertar marca agua a un pdf
            </action>

        </release>

        <release version="1.0.2" date="2020-12-23" description="Metodo para desencriptar pdf y metodo integrado de desencriptar y separar">
            <action dev="mrojas" type="add">
                Método para desencriptar pdf y metodo integrado de desencriptar y separar
            </action>
        </release>

        <release version="1.0.1" date="2020-12-21" description="Metodo para combinar, separar pdf">
            <action dev="jmiraval" type="add">
               Método para combinar, separar pdf
            </action>
        </release>
    </body>
</document>